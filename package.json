{
  "name": "server-template",
  "version": "1.0.0",
  "description": "Simple express server with sequelize, pg and jwts",
  "main": "index.js",
  "engines": {
    "node": ">=22.20.0",
    "npm": ">=10"
  },
  "scripts": {
    "start": "node index.js",
    "dev": "env DELAY=1000 npx nodemon .",
    "initdev": "npm run migrate && npm run seed",
    "migrate": "npx sequelize-cli db:migrate",
    "seed": "npx sequelize-cli db:seed:all",
    "resetDB": "npx sequelize-cli db:migrate:undo:all && npm run initdev",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "template",
    "express",
    "jwt",
    "sequelize"
  ],
  "author": "Rein Op 't Land",
  "license": "MIT",
  "dependencies": {
    "bcrypt": "^5.1.1",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "jsonwebtoken": "^9.0.2",
    "pg": "^8.11.5",
    "sequelize": "^6.37.1",
    "sequelize-cli": "^6.6.1"
  },
  "devDependencies": {
    "axios": "^1.7.2",
    "nodemon": "^3.1.4"
  }
}
